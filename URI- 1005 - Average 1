import java.util.Scanner;

/**
 *
 * @author Ashif Sarker
 */
public class Main {
    public static void main(String[] args) {
        float a,b,sum;
        Scanner in = new Scanner (System.in);
        a=in.nextFloat();
        b=in.nextFloat();
        sum=(float)(((a*3.5)+(b*7.5))/(3.5+7.5));
        String MEDIA = String.format("MEDIA = %,.5f", sum);
       
        System.out.print(MEDIA + "\n" );
    }
    
}
