import java.util.Scanner;

/**
 *
 * @author Ashif Sarker
 */
public class Main {
    public static void main(String[] args) {
         float a,b,c,sum;
        Scanner in =new Scanner (System.in);
        a=in.nextFloat();
        b=in.nextFloat();
        c=in.nextFloat();
        sum=(float)(((a*2)+(b*3)+(c*5))/(2+3+5));
        String MEDIA = String.format("MEDIA = %,.1f", sum);
        System.out.print(MEDIA+ "\n");
    }
}
