import java.util.Scanner;

/**
 *
 * @author Ashif Sarker
 */
public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner (System.in);
      int A,B,sum;
      A=in.nextInt();
      B=in.nextInt();
      sum=A*B;
      System.out.println("PROD = "+sum);
    }
    
}
